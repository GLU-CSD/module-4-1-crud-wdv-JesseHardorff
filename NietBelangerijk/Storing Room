function Deleting() {
  Box1Available = true
  Box2Available = true
  Box3Available = true
  Box4Available = true
  Box5Available = true

  TitleV1 = "";
  DescriptionV1 = "";
  PriceV1 = "";
  TotalPriceV1 = "";
  ImgV1 = "";
  
  TitleV2 = "";
  DescriptionV2 = "";
  PriceV2 = "";
  TotalPriceV2 = "";
  ImgV2 = "";
  
  TitleV3 = "";
  DescriptionV3 = "";
  PriceV3 = "";
  TotalPriceV3 = "";
  ImgV3 = "";

  TitleV4 = "";
  DescriptionV4 = "";
  PriceV4 = "";
  TotalPriceV4 = "";
  ImgV4 = "";

  TitleV5 = "";
  DescriptionV5 = "";
  PriceV5 = "";
  TotalPriceV5 = "";
  ImgV5 = "";

  // Update de DOM om de lege waarden te weerspiegelen
  document.getElementById("title1").innerText = TitleV1;
  document.getElementById("description1").innerText = DescriptionV1;
  document.getElementById("price1").innerText = PriceV1;
  document.getElementById("totalprice1").innerText = TotalPriceV1;
  document.getElementById("productImage1").setAttribute("src", ImgV1);

  localStorage.removeItem("TitleV1");
  localStorage.removeItem("DescriptionV1");
  localStorage.removeItem("PriceV1");
  localStorage.removeItem("TotalPriceV1");
  localStorage.removeItem("ImgV1");

  document.getElementById("title2").innerText = TitleV2;
  document.getElementById("description2").innerText = DescriptionV2;
  document.getElementById("price2").innerText = PriceV2;
  document.getElementById("totalprice2").innerText = TotalPriceV2;
  document.getElementById("productImage2").setAttribute("src", ImgV2);

  localStorage.removeItem("TitleV2");
  localStorage.removeItem("DescriptionV2");
  localStorage.removeItem("PriceV2");
  localStorage.removeItem("TotalPriceV2");
  localStorage.removeItem("ImgV2");

  document.getElementById("title3").innerText = TitleV3;
  document.getElementById("description3").innerText = DescriptionV3;
  document.getElementById("price3").innerText = PriceV3;
  document.getElementById("totalprice3").innerText = TotalPriceV3;
  document.getElementById("productImage3").setAttribute("src", ImgV3);

  localStorage.removeItem("TitleV3");
  localStorage.removeItem("DescriptionV3");
  localStorage.removeItem("PriceV3");
  localStorage.removeItem("TotalPriceV3");
  localStorage.removeItem("ImgV3");

  document.getElementById("title4").innerText = TitleV4;
  document.getElementById("description4").innerText = DescriptionV4;
  document.getElementById("price4").innerText = PriceV4;
  document.getElementById("totalprice4").innerText = TotalPriceV4;
  document.getElementById("productImage4").setAttribute("src", ImgV4);

  localStorage.removeItem("TitleV4");
  localStorage.removeItem("DescriptionV4");
  localStorage.removeItem("PriceV4");
  localStorage.removeItem("TotalPriceV4");
  localStorage.removeItem("ImgV4");

  document.getElementById("title5").innerText = TitleV5;
  document.getElementById("description5").innerText = DescriptionV5;
  document.getElementById("price5").innerText = PriceV5;
  document.getElementById("totalprice5").innerText = TotalPriceV5;
  document.getElementById("productImage5").setAttribute("src", ImgV5);

  localStorage.removeItem("TitleV5");
  localStorage.removeItem("DescriptionV5");
  localStorage.removeItem("PriceV5");
  localStorage.removeItem("TotalPriceV5");
  localStorage.removeItem("ImgV5");

  
}





