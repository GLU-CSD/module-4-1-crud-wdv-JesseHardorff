var DescriptionV3 = localStorage.getItem("DescriptionV3");
var PriceV3 = localStorage.getItem("PriceV3");
var TotalPriceV3 = localStorage.getItem("TotalPriceV3");
var ImgV3 = localStorage.getItem("ImgV3");

