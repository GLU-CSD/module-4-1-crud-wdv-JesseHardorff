var DescriptionV2 = localStorage.getItem("DescriptionV2");
var PriceV2 = localStorage.getItem("PriceV2");
var TotalPriceV2 = localStorage.getItem("TotalPriceV2");
var ImgV2 = localStorage.getItem("ImgV2");

